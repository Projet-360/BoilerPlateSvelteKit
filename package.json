    "sass": "latest",
    "smooth-scrollbar": "latest",
    "svelte": "latest",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "latest",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
