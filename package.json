    "autoprefixer": "latest",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gsap": "latest",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "latest",
    "smooth-scrollbar": "latest",
    "svelte": "latest",
    "svelte-preprocess": "latest",
    "vite-plugin-sass": "latest"
  },
  "type": "module",
  "dependencies": {
    "cookie-parser": "^1.4.6",
