{
  "name": "boilersvelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"cd ./backend && npm run dev\" \"vite dev\" \"npm run prettier:watch\"",
    "prettier:watch": "chokidar '**/*.js' '**/*.svelte' '**/*.css' --command 'npm run prettier -- {path}'",
