{
  "name": "boilersvelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"cd ./backend && npm run dev\" \"vite dev\" \"npm run prettier:watch\" \"start cmd.exe /K .\\utils\\mail\\start-mailhog.bat\"",
    "prettier:watch": "chokidar '**/*.js' '**/*.ts' '**/*.svelte' '**/*.scss' --command 'npm run prettier -- {path}'",
    "prettier": "prettier --write",
    "build": "vite build",
    "preview": "concurrently \"cd ./backend && npm run dev\" \"vite preview\"",
    "test": "playwright test",
    "test:unit": "vitest",
    "cleardb": "npm --prefix ./backend run cleardb"
  },
  "browserslist": [
    "> 5%"
  ],
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@rollup/plugin-terser": "^0.4.4",
