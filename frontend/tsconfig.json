{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"types": ["src/app.d.ts"]
	},
	"include": [
		"./src/**/*.ts",
		"./src/**/*.d.ts",
		"./src/app.d.ts",
	]
}
