{
	"compilerOptions": {
		"paths": {
			"$db": [
				"../src/db"
			],
			"$db/*": [
				"../src/db/*"
			],
			"$components": [
				"../src/components"
			],
			"$components/*": [
				"../src/components/*"
			],
			"$message": [
				"../src/utils/message"
			],
			"$message/*": [
				"../src/utils/message/*"
			],
			"$utils": [
				"../src/utils"
			],
			"$utils/*": [
				"../src/utils/*"
			],
			"$stores": [
				"../src/stores"
			],
			"$stores/*": [
				"../src/stores/*"
			],
			"$UITools": [
				"../src/UITools"
			],
			"$UITools/*": [
				"../src/UITools/*"
			],
			"$language": [
				"../src/language"
			],
			"$language/*": [
				"../src/language/*"
			],
			"$three": [
				"../src/components/three"
			],
			"$three/*": [
				"../src/components/three/*"
			],
			"$services": [
				"../src/services"
			],
			"$services/*": [
				"../src/services/*"
			],
			"$lib": [
				"../src/lib"
			],
			"$lib/*": [
				"../src/lib/*"
			],
			"$api": [
				"../src/api"
			],
			"$api/*": [
				"../src/api/*"
			]
		},
		"rootDirs": [
			"..",
			"./types"
		],
		"importsNotUsedAsValues": "error",
		"isolatedModules": true,
		"preserveValueImports": true,
		"lib": [
			"esnext",
			"DOM",
			"DOM.Iterable"
		],
		"moduleResolution": "node",
		"module": "esnext",
		"target": "esnext",
		"ignoreDeprecations": "5.0"
	},
	"include": [
		"ambient.d.ts",
		"./types/**/$types.d.ts",
		"../vite.config.ts",
		"../src/**/*.js",
		"../src/**/*.ts",
		"../src/**/*.svelte",
		"../tests/**/*.js",
		"../tests/**/*.ts",
		"../tests/**/*.svelte"
	],
	"exclude": [
		"../node_modules/**",
		"./[!ambient.d.ts]**",
		"../src/service-worker.js",
		"../src/service-worker.ts",
		"../src/service-worker.d.ts"
	]
}