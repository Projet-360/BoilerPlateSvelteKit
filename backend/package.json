 "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "concurrently \"cross-env NODE_ENV=development nodemon --watch \\\"**/*.ts\\\" --exec 'node --loader ts-node/esm' ./index.ts\" \"nodemon --watch \\\"./**/*.ts\\\" --exec \\\"prettier --write \"./**/*.ts\\\"\"",
    "cleardb": "ts-node clear-db.ts",
    "docs": "typedoc --out docs",
    "format": "prettier --write \"./**/*.ts\"",
    "prettier:watch": "nodemon --watch \"./**/*.ts\" --exec \"prettier --write \"./**/*.ts\""
  },
