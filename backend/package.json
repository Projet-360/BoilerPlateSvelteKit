    "express": "^4.18.2",
    "express-rate-limit": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "nodemailer": "^6.9.5",
    "winston": "^3.10.0"
  },
  "devDependencies": {
